{
  "name": "Parse Server",
  "description": "Parse Server for heroku",
  "repository": "https://github.com/yongjhih/heroku-parse-server",
  "logo": "https://avatars0.githubusercontent.com/u/1294580?v=3&s=200",
  "keywords": ["node", "express", "parse"],
  "env": {
    "APP_ID": {
      "description": "A unique identifier for your app.",
      "value": "myAppId"
    },
    "MASTER_KEY": {
      "description": "A key that overrides all permissions. Keep this secret.",
      "value": "myMasterKey"
    },
    "JAVASCRIPT_KEY": {
      "description": "Key",
      "value": "myKey"
    },
    "REST_API_KEY": {
      "description": "Key",
      "value": "myKey"
    },
    "FACEBOOK_APP_IDS": {
      "description": "ids",
      "value": "myId"
    },
    "PARSE_MOUNT": {
      "description": "Configure Parse API route.",
      "value": "/parse"
    },
    "SERVER_URL": {
      "description": "URL to connect to your Heroku instance (leave as http://localhost)",
      "value": "http://localhost/parse"
    }
  },
  "image": "heroku/nodejs",
  "addons": ["mongolab"]
}
